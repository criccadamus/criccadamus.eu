{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "criccadamus-eu",
  "workers_dev": false,
  "preview_urls": true,
  "compatibility_date": "2025-11-11",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist",
  },
  "observability": {
    "enabled": false,
  },
  "routes": [
    {
      "pattern": "criccadamus.eu",
      "custom_domain": true,
    },
  ],
}
